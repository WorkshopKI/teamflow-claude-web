{
  "name": "@teamflow/workflow-engine",
  "version": "0.1.0",
  "description": "Visual workflow automation engine for TeamFlow AI",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./src/scheduler": {
      "types": "./dist/scheduler.d.ts",
      "default": "./dist/scheduler.js"
    },
    "./src/event-trigger": {
      "types": "./dist/event-trigger.d.ts",
      "default": "./dist/event-trigger.js"
    },
    "./src/templates": {
      "types": "./dist/templates.d.ts",
      "default": "./dist/templates.js"
    },
    "./src/import-export": {
      "types": "./dist/import-export.d.ts",
      "default": "./dist/import-export.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@teamflow/types": "*",
    "nanoid": "^5.0.4"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
